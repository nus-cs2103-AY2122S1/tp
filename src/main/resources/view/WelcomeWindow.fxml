<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Scene?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.image.ImageView?>
<?import java.net.URL?>

<fx:root resizable="false" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" type="javafx.stage.Stage">
    <scene>
        <Scene>
            <stylesheets>
                <URL value="@WelcomeWindow.css" />
            </stylesheets>

            <AnchorPane prefHeight="250" prefWidth="290" style="-fx-background-color: #121212#121212;">
                <VBox>
                    <children>
                        <ImageView fx:id="appLogo" fitHeight="240.0" fitWidth="290.0" pickOnBounds="true" preserveRatio="true"/>
                        <Label fx:id="appTagLine" text="Label">
                        </Label>
                    </children>
                </VBox>
            </AnchorPane>
        </Scene>
    </scene>
</fx:root>
