<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.layout.HBox?>
<?import javafx.scene.control.Label?>
<VBox styleClass="pane-with-border" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
  <padding>
    <Insets bottom="10" left="10" right="10" top="10" />
  </padding>
  <HBox fx:id="titleBox" alignment="CENTER" prefWidth="100.0" styleClass="student_title_box" VBox.vgrow="SOMETIMES">
    <children>
      <Label fx:id="personListTitle" alignment="CENTER" styleClass="cell_medium_large_label" text="Students" wrapText="true">
        <padding>
          <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
        </padding></Label>
    </children>
  </HBox>
  <ListView fx:id="personListView" VBox.vgrow="ALWAYS">
    <placeholder>
      <Label fx:id="placeholder" wrapText="true"/>
    </placeholder>
  </ListView>
</VBox>
