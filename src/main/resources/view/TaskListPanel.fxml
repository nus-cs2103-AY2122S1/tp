<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="vbox" maxHeight="Infinity" maxWidth="Infinity" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label fx:id="taskListPanelHeader" prefWidth="${taskListPanelHeader.parent.width}" text="Task List" />
      <ListView fx:id="taskListView" VBox.vgrow="ALWAYS" />
   </children>
</VBox>
