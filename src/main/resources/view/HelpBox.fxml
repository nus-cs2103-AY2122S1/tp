                <Insets bottom="10"/>
            </padding>
        </HBox>
        <HBox alignment="CENTER_LEFT">
            <Label styleClass="cell_small_label, cell_label_width_400"
                   text="Summary"/>
            <Label styleClass="cell_small_label"
                   text="summary"/>
            <padding>
                <Insets bottom="10"/>
            </padding>
        </HBox>
        <HBox alignment="CENTER_LEFT">
            <Label styleClass="cell_small_label, cell_label_width_400"
                   text="Clear ezFoodie Data"/>
            <Label styleClass="cell_small_label"
                   text="clear"/>
            <padding>
                <Insets bottom="10"/>
            </padding>
        </HBox>
        <HBox alignment="CENTER_LEFT">
            <Label fx:id="url"
                   styleClass="cell_small_label, cell_label_width_400"
                   text="\$url"/>
            <Button fx:id="copyButton"
                    mnemonicParsing="false"
                    onAction="#copyUrl"
                    text="Copy URL"/>
        </HBox>
    </VBox>
</StackPane>
