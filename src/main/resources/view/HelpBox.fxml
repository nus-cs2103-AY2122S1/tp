        <HBox alignment="CENTER_LEFT">
            <Label styleClass="cell_small_label, cell_label_width_400"
                   text="Find Members by Name"/>
            <Label fx:id="findMembersByName"
                   styleClass="cell_small_label"
                   text="\$findMembersByName"/>
        </HBox>
        <HBox alignment="CENTER_LEFT">
            <Label styleClass="cell_small_label, cell_label_width_400"
