<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<StackPane styleClass="stack-pane" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
    <VBox fx:id="helpBox" alignment="CENTER_LEFT"
          minHeight="105"
          GridPane.columnIndex="0">
        <padding>
            <Insets top="5" right="5" bottom="5" left="15"/>
        </padding>
        <HBox alignment="CENTER_LEFT">
            <Label styleClass="cell_small_label, cell_label_width_400"
                   text="Exit Program"/>
            <Label styleClass="cell_small_label"
                   text="exit"/>
            <padding>
                <Insets bottom="10"/>
            </padding>
        </HBox>
        <HBox alignment="CENTER_LEFT">
            <Label styleClass="cell_small_label, cell_label_width_400"
                   text="Login as Manager"/>
            <Label fx:id="login"
                   styleClass="cell_small_label"
                   text="\$login"/>
        </HBox>
        <HBox alignment="CENTER_LEFT">
            <Label styleClass="cell_small_label, cell_label_width_400"
                   text="Logout as Manager"/>
            <Label styleClass="cell_small_label"
                   text="logout"/>
            <padding>
                <Insets bottom="10"/>
            </padding>
        </HBox>
        <HBox alignment="CENTER_LEFT">
            <Label styleClass="cell_small_label, cell_label_width_400"
                   text="Set Password"/>
            <Label fx:id="setPassword" styleClass="cell_small_label"
                   text="\$setPassword"/>
            <padding>
                <Insets bottom="10"/>
            </padding>
        </HBox>
        <HBox alignment="CENTER_LEFT">
            <Label styleClass="cell_small_label, cell_label_width_400"
                   text="List Members"/>
            <Label styleClass="cell_small_label"
                   text="list -mem/"/>
            <padding>
                <Insets bottom="10"/>
            </padding>
        </HBox>
        <HBox alignment="CENTER_LEFT">
            <Label styleClass="cell_small_label, cell_label_width_400"
                   text="Add New Member"/>
            <Label fx:id="addNewMember"
                   styleClass="cell_small_label"
                   text="\$addNewMember"/>
            <padding>
                <Insets bottom="10"/>
            </padding>
        </HBox>
        <HBox alignment="CENTER_LEFT">
            <Label styleClass="cell_small_label, cell_label_width_400"
                   text="Delete Member by Member ID"/>
            <Label fx:id="deleteMemberByMemberId"
                   styleClass="cell_small_label"
                   text="\$deleteMemberByMemberId"/>
        </HBox>
        <HBox alignment="CENTER_LEFT">
            <Label styleClass="cell_small_label, cell_label_width_400"
