            <Label fx:id="findMembersByRegistrationDate"
                   styleClass="cell_small_label"
                   text="\$findMembersByRegistrationDate"/>
            <padding>
                <Insets bottom="10"/>
            </padding>
        </HBox>
        <HBox alignment="CENTER_LEFT">
            <Label styleClass="cell_small_label, cell_label_width_400"
                   text="View Member Profile"/>
            <Label fx:id="viewMemberProfile"
                   styleClass="cell_small_label"
                   text="\$viewMemberProfile"/>
            <padding>
                <Insets bottom="10"/>
            </padding>
        </HBox>
        <HBox alignment="CENTER_LEFT">
            <Label styleClass="cell_small_label, cell_label_width_400"
                   text="Edit Member by Member ID"/>
            <Label fx:id="editMemberByMemberId"
                   styleClass="cell_small_label"
                   text="\$editMemberByMemberId"/>
        </HBox>
        <HBox alignment="CENTER_LEFT">
            <Label styleClass="cell_small_label, cell_label_width_400"
                   text="Edit Member by Index Number"/>
            <Label fx:id="editMemberByIndexNumber"
                   styleClass="cell_small_label"
                   text="\$editMemberByIndexNumber"/>
            <padding>
                <Insets bottom="10"/>
            </padding>
        </HBox>
        <HBox alignment="CENTER_LEFT">
            <Label styleClass="cell_small_label, cell_label_width_400"
                   text="Add Transaction by Member ID"/>
            <Label fx:id="addTransactionByMemberId"
                   styleClass="cell_small_label"
                   text="\$addTransactionByMemberId"/>
            <padding>
                <Insets bottom="10"/>
            </padding>
        </HBox>
        <HBox alignment="CENTER_LEFT">
            <Label styleClass="cell_small_label, cell_label_width_400"
                   text="Edit Transaction by Member ID and Transaction ID"/>
            <Label fx:id="editTransactionByMemberIdTransactionId"
                   styleClass="cell_small_label"
                   text="\$editTransactionByMemberIdTransactionId"/>
            <padding>
                <Insets bottom="10"/>
            </padding>
        </HBox>
        <HBox alignment="CENTER_LEFT">
            <Label styleClass="cell_small_label, cell_label_width_400"
                   text="Delete Transaction by Member ID and Transaction ID"/>
            <Label fx:id="deleteTransactionByMemberIdTransactionId"
                   styleClass="cell_small_label"
