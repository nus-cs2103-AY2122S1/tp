<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<HBox prefHeight="38.0" prefWidth="438.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1">
   <HBox spacing="10.0" HBox.hgrow="ALWAYS">
      <HBox.margin>
         <Insets left="10.0" />
      </HBox.margin>
                        <Label fx:id="welcomeLabel" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" styleClass="label-header" text="Welcome" textAlignment="CENTER">
                     <font>
                        <Font size="18.0" />
                     </font>
                  </Label>
         <Label fx:id="usernameLabel" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" styleClass="label-header" text="username" textAlignment="CENTER" wrapText="true" HBox.hgrow="ALWAYS">
            <font>
               <Font size="18.0" />
            </font>
         </Label>

   </HBox>
               <Button fx:id="addFriendButton" alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="Add Friend" textAlignment="CENTER" HBox.hgrow="NEVER">
         <HBox.margin>
            <Insets bottom="10.0" right="10.0" />
         </HBox.margin></Button>
</HBox>
