<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.layout.HBox?>

<TitledPane fx:id="commandTitle" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" styleClass="commandTitle">
    <stylesheets>
        <URL value="@CommandSummaryWindow.css" />
    </stylesheets>
    <content>
        <VBox>
            <HBox>
                <Label styleClass="formatLabel">
                    Format:
                </Label>
                <Label fx:id="format"/>
            </HBox>
            <HBox>
                <Label styleClass="descriptionLabel">
                    Description:
                </Label>
                <Label fx:id="description"/>
            </HBox>
            <HBox>
                <Label styleClass="examplesLabel">
                    Examples:
                </Label>
                <Label fx:id="examples"/>
            </HBox>
        </VBox>
    </content>
</TitledPane>
