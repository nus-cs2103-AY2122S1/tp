<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>

<fx:root spacing="1" alignment="TOP_LEFT" type="javafx.scene.layout.HBox" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <ImageView fx:id="displayPicture" styleClass="cell_small_label" fitHeight="15.0" fitWidth="15.0" pickOnBounds="true" preserveRatio="true" />
        <Label fx:id="component" text="Label" wrapText="true" alignment="TOP_CENTER" styleClass="cell_small_label" textAlignment="LEFT"/>
    </children>
</fx:root>
