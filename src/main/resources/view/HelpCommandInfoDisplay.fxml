   <children>
      <VBox fx:id="commandInfoContainer">
         <padding>
            <Insets left="8.0" top="8.0" right="8.0" bottom="8.0" />
         </padding>

         <TextFlow fx:id="commandInfo" />
      </VBox>
   </children>
</StackPane>
