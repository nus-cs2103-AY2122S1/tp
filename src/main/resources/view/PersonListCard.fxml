<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.VBox?>

<HBox id="cardPane" fx:id="cardPane" xmlns="http://javafx.com/javafx/11" xmlns:fx="http://javafx.com/fxml/1">
 <VBox alignment="CENTER_LEFT" maxHeight="-Infinity" minWidth="-Infinity" prefWidth="200.0" HBox.hgrow="ALWAYS">
   <padding>
     <Insets bottom="10.0" left="15.0" right="5.0" top="10.0" />
   </padding>
   <HBox alignment="CENTER_LEFT" spacing="5">
     <Label fx:id="id" styleClass="cell_big_label">
       <minWidth>
         <!-- Ensures that the label text is never truncated -->
         <Region fx:constant="USE_PREF_SIZE" />
       </minWidth>
     </Label>
     <Label fx:id="name" styleClass="cell_big_label" text="\$first" wrapText="true" />
   </HBox>
   <FlowPane fx:id="tags" maxWidth="-Infinity" prefWidth="200.0">
         <VBox.margin>
            <Insets bottom="3.0" top="3.0" />
         </VBox.margin></FlowPane>
      <FlowPane fx:id="insurances" layoutX="25.0" layoutY="68.0" />
   <Label fx:id="phone" styleClass="cell_small_label" text="\$phone" wrapText="true" />
   <Label fx:id="address" styleClass="cell_small_label" text="\$address" wrapText="true" />
   <Label fx:id="email" styleClass="cell_small_label" text="\$email" wrapText="true" />
   <Label fx:id="revenue" styleClass="cell_small_label" text="\$revenue" wrapText="true" />
   <Label fx:id="meeting" styleClass="cell_small_label" text="\$meeting" wrapText="true" />
   <Label fx:id="note" styleClass="cell_small_label" text="\$note" wrapText="true" />
 </VBox>
</HBox>
