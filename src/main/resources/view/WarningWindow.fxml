<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Scene?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.TextFlow?>
<?import javafx.stage.Stage?>

<Stage xmlns="http://javafx.com/javafx/11" xmlns:fx="http://javafx.com/fxml/1">
    <scene>
        <Scene stylesheets="@GeneralWindow.css">
            <AnchorPane prefHeight="151.0" prefWidth="368.0">
            <children>
               <TextFlow fx:id="textFlow" layoutX="19.0" layoutY="29.0" prefHeight="71.0" prefWidth="330.0" />
               <ButtonBar layoutY="112.0" prefHeight="40.0" prefWidth="368.0" style="-fx-background-color: #ECECEC;">
                  <buttons>
                     <Button mnemonicParsing="false" onAction="#onClickContinue" text="Continue" translateX="-10.0">
                        <font>
                           <Font name="Calibri" size="12.0" />
                        </font>
                        <opaqueInsets>
                           <Insets />
                        </opaqueInsets>
                     </Button>
                     <Button fx:id="cancelButton" mnemonicParsing="false" onAction="#onClickCancel" text="Cancel" translateX="-10.0">
                        <font>
                           <Font name="Calibri" size="12.0" />
                        </font>
                        <opaqueInsets>
                           <Insets />
                        </opaqueInsets>
                     </Button>
                  </buttons>
               </ButtonBar>
            </children>
         </AnchorPane>
        </Scene>
    </scene>
</Stage>
