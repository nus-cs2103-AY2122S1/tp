<VBox styleClass="background-with-border-dark" stylesheets="@AdvyzeTheme.css" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1">
   <GridPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="-Infinity" prefHeight="27.0" styleClass="table-header" stylesheets="@AdvyzeTheme.css" VBox.vgrow="NEVER">
     <columnConstraints>
         <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
       <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
       <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
     </columnConstraints>
     <rowConstraints>
       <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
     </rowConstraints>
      <children>
         <Label styleClass="light_text" stylesheets="@AdvyzeTheme.css" text="Description">
            <padding>
               <Insets left="5.0" />
            </padding>
            <GridPane.margin>
               <Insets />
            </GridPane.margin></Label>
         <Label styleClass="light_text" stylesheets="@AdvyzeTheme.css" text="Date" GridPane.columnIndex="1">
            <GridPane.margin>
               <Insets />
            </GridPane.margin>
            <padding>
               <Insets left="5.0" />
            </padding></Label>
         <Label styleClass="light_text" stylesheets="@AdvyzeTheme.css" text="Time" GridPane.columnIndex="2">
            <GridPane.margin>
               <Insets />
            </GridPane.margin>
            <padding>
               <Insets left="5.0" />
            </padding></Label>
      </children>
   </GridPane>
  <ListView fx:id="scheduleListView" stylesheets="@AdvyzeTheme.css" VBox.vgrow="ALWAYS" />
</VBox>
