<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.geometry.Insets?>

<GridPane styleClass="status-bar" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
    <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10" />
    </columnConstraints>
    <padding>
        <Insets top="5" right="5" bottom="5" left="10" />
    </padding>
    <Label fx:id="personType"
           style=" -fx-font-weight: bold; -fx-font-size: 18px; -fx-text-fill: coral; -fx-margin-left: 10" />
</GridPane>

