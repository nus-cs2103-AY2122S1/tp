<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Scene?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.TextFlow?>
<?import javafx.stage.Stage?>

<Stage xmlns="http://javafx.com/javafx/11" xmlns:fx="http://javafx.com/fxml/1">
    <scene>
        <Scene stylesheets="@GeneralWindow.css">
            <AnchorPane prefHeight="151.0" prefWidth="385.0">
            <children>
               <TextFlow fx:id="textFlow" layoutX="14.0" layoutY="27.0" prefHeight="71.0" prefWidth="373.0" />
               <ButtonBar layoutY="112.0" prefHeight="40.0" prefWidth="400.0" style="-fx-background-color: #ECECEC;">
                  <buttons>
                     <Button mnemonicParsing="false" onAction="#onClickSave" text="Save" translateX="-10.0">
                        <font>
                           <Font name="Calibri" size="12.0" />
                        </font>
                        <opaqueInsets>
                           <Insets />
                        </opaqueInsets>
                     </Button>
                     <Button mnemonicParsing="false" onAction="#onClickDoNotSave" text="Don't Save" translateX="-10.0">
                        <font>
                           <Font name="Calibri" size="12.0" />
                        </font>
                        <opaqueInsets>
                           <Insets />
                        </opaqueInsets>
                     </Button>
                     <Button mnemonicParsing="false" onAction="#onClickCancel" prefHeight="22.0" text="Return to note" translateX="-10.0">
                        <opaqueInsets>
                           <Insets />
                        </opaqueInsets>
                        <font>
                           <Font name="Calibri" size="12.0" />
                        </font>
                     </Button>
                  </buttons>
               </ButtonBar>
            </children>
         </AnchorPane>
        </Scene>
    </scene>
</Stage>
